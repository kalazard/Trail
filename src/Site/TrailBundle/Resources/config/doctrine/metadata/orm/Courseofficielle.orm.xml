<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Courseofficielle" table="courseofficielle">
    <indexes>
      <index name="fk_courseofficielle_evenement_idx" columns="evenement"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="siteUtl" type="string" column="site_utl" length="255" nullable="false"/>
    <many-to-one field="evenement" target-entity="Evenement">
      <join-columns>
        <join-column name="evenement" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
