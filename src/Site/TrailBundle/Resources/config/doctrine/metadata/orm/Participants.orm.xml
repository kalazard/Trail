<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Participants" table="participants">
    <indexes>
      <index name="fk_participants_membre_idx" columns="membre"/>
      <index name="fk_participants_evenement_idx" columns="evenement"/>
      <index name="fk_participants_participation_idx" columns="participation"/>
    </indexes>
    <id name="evenement" association-key="true"/>
    <id name="membre" association-key="true"/>
    <one-to-one field="evenement" target-entity="Evenement">
      <join-columns>
        <join-column name="evenement" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="membre" target-entity="Membre">
      <join-columns>
        <join-column name="membre" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
    <many-to-one field="participation" target-entity="Participation">
      <join-columns>
        <join-column name="participation" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
